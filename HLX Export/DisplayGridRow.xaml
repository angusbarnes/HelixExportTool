<UserControl x:Class="HLXExport.DisplayGridRow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HLXExport"
             mc:Ignorable="d" Height="27" Width="465">
    <Grid>
        <StackPanel x:Name="Row" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Border BorderBrush="#FF656565" BorderThickness="0,0,0,1" ClipToBounds="True"/>
    </Grid>
</UserControl>
